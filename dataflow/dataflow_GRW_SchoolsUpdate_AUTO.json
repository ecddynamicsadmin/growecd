{
	"name": "dataflow_GRW_SchoolsUpdate_AUTO",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "storage_GRW_SchoolsUpdate_AUTO_Json",
						"type": "DatasetReference"
					},
					"name": "jsonschoolupdatefile"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "storage_GRW_SchoolsUpdate_AUTO_csv",
						"type": "DatasetReference"
					},
					"name": "sinktocsv"
				}
			],
			"transformations": [
				{
					"name": "removeformatting"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'documentPerLine') ~> jsonschoolupdatefile",
				"jsonschoolupdatefile derive(test = 5) ~> removeformatting",
				"removeformatting sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['persons.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sinktocsv"
			]
		}
	}
}