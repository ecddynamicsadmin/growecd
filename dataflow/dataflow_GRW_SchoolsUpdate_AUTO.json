{
	"name": "dataflow_GRW_SchoolsUpdate_AUTO",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "storage_GRW_SchoolsUpdate_AUTO_Json",
						"type": "DatasetReference"
					},
					"name": "jsonschoolupdatefile"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "storage_GRW_SchoolsUpdate_AUTO_csv",
						"type": "DatasetReference"
					},
					"name": "sinktocsv"
				}
			],
			"transformations": [
				{
					"name": "removeformatting"
				}
			],
			"scriptLines": [
				"source(output(",
				"          address as string,",
				"          agreement_type as string,",
				"          break_even_fee as short,",
				"          business_mentor_email as string,",
				"          business_mentor_id as short,",
				"          business_mentor_name as string,",
				"          business_mentor_phone_number as string,",
				"          capacity as short,",
				"          city_id as short,",
				"          closing_hours_friday as string,",
				"          closing_hours_monday_to_thursday as string,",
				"          contract as string,",
				"          contract_end_date as string,",
				"          contract_start_date as string,",
				"          created_at as string,",
				"          created_by as short,",
				"          ecd_code as string,",
				"          ecd_start_date as string,",
				"          education_mentor_email as string,",
				"          education_mentor_id as short,",
				"          education_mentor_name as string,",
				"          education_mentor_phone_number as string,",
				"          form_11 as boolean,",
				"          franchisee_id as short,",
				"          grow_id as string,",
				"          hourly_late_or_early_fee as short,",
				"          hr_email as string,",
				"          hr_staff_code as string,",
				"          id as short,",
				"          inventory_group_id as string,",
				"          is_caregiver_access as boolean,",
				"          is_learner_assessment as boolean,",
				"          is_learner_attendance as boolean,",
				"          is_learner_payment as boolean,",
				"          learner_capacity as short,",
				"          logo as string,",
				"          monthly_salaries as double,",
				"          name as string,",
				"          number_of_classrooms as short,",
				"          opening_hours_monday_to_friday as string,",
				"          operating_days as string,",
				"          owner_id as short,",
				"          program_status as string,",
				"          province_id as short,",
				"          region_id as short,",
				"          school_commencement_date as string,",
				"          school_id as string,",
				"          school_status as string,",
				"          segment as string,",
				"          updated_at as string,",
				"          updated_by as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'documentPerLine') ~> jsonschoolupdatefile",
				"jsonschoolupdatefile derive(address = replace(replace(address, '\\n', ','), '\\r', ',')) ~> removeformatting",
				"removeformatting sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['vw_schools_rpt_final_null_op.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sinktocsv"
			]
		}
	}
}